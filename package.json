    "build-client": "npm --prefix blog-client run build",
    "install-server": "npm --prefix blog-server install",
    "install-client": "npm --prefix blog-client install",
    "install-all": "npm run install-server && npm run install-client && npm install",
    "build": "npm run build-server && npm run build-client",
    "test-client": "npm --prefix blog-client run test",
    "test-server": "npm --prefix blog-server run test",
    "start-cypress-mode": "npm --prefix blog-server run cypress-mode",
    "lint-client": "npm --prefix blog-client run eslint",
    "lint-server": "npm --prefix blog-server run eslint",
    "cypress": "cypress run --spec \"cypress/e2e/blog_app.cy.js\""
